RewriteEngine On
RewriteBase /parlor/demo_1/

# Redirect .php URLs to clean URLs
RewriteCond %{THE_REQUEST} \s/([^.]+)\.php[\s?]
RewriteRule ^ %1 [R=301,L]

# Serve clean URLs
RewriteRule ^$ index.php [L]
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^/]+)/?$ $1.php [L]
